*{
	margin:0;
	padding:0;
}
#bigbox{
	width:999px;
	height:1508px;
	margin:0 auto;
	border:1px solid red;
	overflow:hidden;

}
#header,#nav,#banner,#section,#wl_park,#footernav,#footer{
	width:999px;
}
#header{
	height:100px;
	
}
#header a img{
	float:left;
}
#header input{
	float:right;
	margin:38px 19px 0 0;
	width:200px;
	height:30px;
	background:#f1f1f1;
	border:0;outline:none;
	text-indent:2em;
}
#header button{
	width:28px;
	height:30px;
	margin:38px -200px 0 0 ;
	float:right;
	border:0;
	background:url(images/02.png) no-repeat center;
}
#nav{
	height:59px;
	background:linear-gradient(to top,#333,#4c1518);
	
}
.span1{
	color:white;
	text-decoration:none;
}
a{
	text-decoration:none;
}
.nav1{
	text-align:center;
	line-height:59px;
}
ul{
	list-style-type:none;
	float:left;
}
.list{
	display:none; 
	position:absolute;
	background:#f1f1f1;
	z-index:10;
}
.nav6{
	border:1px solid black;
}
.nav1{position:relative;
}
.nav1:hover .list{
	display:block;
}
.nav1 .nav6:hover{
	background:white;
}
.nav1:hover{
	background:#d54344;
}
#banner{
	
	overflow:hidden;
	height:463px;
}
.banner{
	width:300px;
	height:180px;
	position:relative;
	top:-275px;
	left:23px;
	background:#ca2722;
	border-radius:0 15% 0 0 ;
	z-index:2;
}
.banner1{
	line-height:66px;
	text-align:center;
	font-size:25px;
	color:white;
}
.banner2{
	text-align:center;
	color:white;
	font-size:15px;
}
section{
	width:20px;
	height:20px;
	background:white;
	color:#ca2722;
	line-height:20px;
	text-align:center;
	border-radius:50%;
	margin:25px 0 0 25px;
	float:left;
	font-size:25px;
}
#banner3{
	line-height:26px;
	margin: 0 0 0 5px;
}

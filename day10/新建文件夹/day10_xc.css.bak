*{
	margin: 0;
	padding: 0;
}
li{
	list-style: none;
}
a{
	text-decoration: none;
}
.bigbox{
	width: 1500px;	
}
.head{
	width:1180px;
	height: 65px;
	margin: 0 auto;
	/*overflow: hidden;*/

}
.head_left{
	margin: 11px 0 11px 0;
	float: left;
}
.head_value{
	float: left;
	height: 65px;
	margin: 20px 0 13px 52px;
}

.head_value input{
	height: 26px;
	width: 370px;
	box-shadow:0 0 5px #8ebefc inset;
	border: none;
	border-radius:5px 0px 0px 5px ;
	border: 1px solid #83befc;
}
.head_value button{
	float: right;
	width: 42px;
	height: 28px;
	background: #a4cbff;
	border: 0;
	border-radius: 0 5px 5px 0;
	outline: none;
	background: url(携程/fangdajing.jpg);
}
.head_value button:hover{
	background: url(携程/1221.png);
}
.head_line{
	width: 408px;
	height: 17px;
	float: left;
	margin: 18px 0 24px 150px;
	
}
.head_line ul{
	position: relative;
	float: left;
	background: #fff;
}
.head_line ul li{
	width: 135px;
	height: 33px;
	color: #4c4c4c;
	float: left;	
}
